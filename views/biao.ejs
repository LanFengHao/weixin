<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <link href="https://cdn.bootcss.com/jquery-mobile/1.4.5/jquery.mobile.min.css" rel="stylesheet">
    <script src="https://cdn.bootcss.com/jquery/2.0.0/jquery.min.js"></script>
    <script src="https://cdn.bootcss.com/jquery-mobile/1.4.5/jquery.mobile.min.js"></script>
    <script src="http://cdn.bootcss.com/socket.io/2.0.3/socket.io.js"></script>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title></title>
  </head>
  <style media="screen">
  #head{
    background-color: #65c294;
  }
  #foot{
    background-color: #65c294;
    padding: 0 20px 0 20px;
    box-sizing: border-box;
  }
    #biao>li{
      text-align: center;
      background-color:#cde6c7;
      padding: 10px 0;
      /*margin: 10px;*/
      /*border-radius: 20px;*/
      border-bottom:10px #f9f9f9 solid;
    }

  </style>
  <body>
    <title>在线用户列表</title>
    <div data-role='page'>
    <div data-role="header" data-position="fixed" id="head"><h1>在线用户列表</h1></div>
    <ul data-role="listview" id="biao">
    </ul>
    <div data-role="footer" data-position="fixed" id="foot"><a href="/">登录</a></div>
  </div>
  <script type="text/javascript">
    var socket = io();
    socket.on('zai xian',function(names) {
      // console.log(names);
      $('#biao').html('')

      for (var i = 0; i < names.length; i++) {
        $('#biao').append('<li>'+names[i]+'</li>')
      }
    })
  </script>
  </body>
</html>
